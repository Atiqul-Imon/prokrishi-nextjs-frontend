# Production Environment Variables for Prokrishi Frontend
# Copy this file to .env.local and fill in your actual values

# API Configuration
NEXT_PUBLIC_API_BASE_URL=https://your-backend-domain.onrender.com/api

# Facebook Configuration (for chat widget)
NEXT_PUBLIC_FACEBOOK_APP_ID=your_facebook_app_id
NEXT_PUBLIC_FACEBOOK_PAGE_ID=your_facebook_page_id

# Analytics (optional)
NEXT_PUBLIC_GA_TRACKING_ID=your_google_analytics_tracking_id

# Other Configuration
NEXT_PUBLIC_SITE_URL=https://your-frontend-domain.vercel.app 